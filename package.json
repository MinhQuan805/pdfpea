{
  "name": "pdf-editor-vue",
  "version": "1.0.0",
  "description": "A browser-based PDF editor built with Vue 3 and vanilla JavaScript",
  "type": "module",
  "main": "src/main.js",
  "scripts": {
    "dev": "vite",
    "build": "vite build --debug && ls -la && ls -la dist",
    "preview": "vite preview",
    "serve": "vite preview --port 8000",
    "lint": "eslint src --ext .js,.vue --fix",
    "format": "prettier --write src/",
    "type-check": "vue-tsc --noEmit"
  },
  "dependencies": {
    "@tailwindcss/vite": "^4.1.11",
    "moveable": "^0.53.0",
    "pdf-lib": "^1.17.1",
    "pdfjs-dist": "5.4.449",
    "svgpath": "^2.6.0",
    "tailwindcss": "^4.1.11",
    "vue": "^3.4.0"
  },
  "devDependencies": {
    "@vitejs/plugin-vue": "^5.0.0",
    "eslint": "^8.57.0",
    "eslint-plugin-vue": "^9.0.0",
    "prettier": "^3.0.0",
    "terser": "^5.26.0",
    "typescript": "^5.0.0",
    "vite": "^5.0.0",
    "vue-tsc": "^1.8.0"
  },
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=9.0.0"
  },
  "keywords": [
    "pdf",
    "editor",
    "vue",
    "javascript",
    "forms",
    "annotations"
  ],
  "author": "",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/user/pdf-editor-vue.git"
  },
  "bugs": {
    "url": "https://github.com/user/pdf-editor-vue/issues"
  },
  "homepage": "https://github.com/user/pdf-editor-vue#readme"
}
